# Database Configuration
MONGODB_URI=mongodb://localhost:27017/dynamic_table_app

# Port Number
PORT=3000

# JWT Secret for Authentication
JWT_SECRET=homework

# External API Keys or Secrets
API_KEY=myapikey
API_SECRET=myapisecret

# Logging Level
LOG_LEVEL=info

# Session and Cookie Configuration
