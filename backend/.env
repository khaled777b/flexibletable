# Environment Type
NODE_ENV=development

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Rate Limiting Configuration
RATE_LIMIT_WINDOW=1
RATE_LIMIT_MAX=100

# Compression Configuration
COMPRESSION_LEVEL=5
